{{! deprecated: move to json formatted model for js variables }}
{{# js_variables }}
    <script>{{{ . }}}</script>
{{/ js_variables }}

{{! new format for js variables }}
<script>
{{# js_vars }}
  {{#unquoted}}
   var {{ name }} = {{{ value }}};
  {{/unquoted}}
  {{^unquoted}}
   var {{ name }} = "{{ value }}";
  {{/unquoted}}
{{/ js_vars }}
</script>

{{# js_files }}
  <script {{# data_main }}data-main="{{.}}"{{/ data_main }} src="{{ path }}{{# cacheBuster }}?{{ cacheBuster }}{{/ cacheBuster }}" {{# type }}type="{{ type }}"{{/ type }}></script>
{{/ js_files}}

{{# js_slsb_ld }}
  <script type="application/ld+json">
  {
    {{# context}}
      "@context": "{{.}}",
    {{/ context}}
    {{# type}}
      "@type": "{{.}}",
    {{/ type}}
    {{# url}}
      "url": "{{.}}",
    {{/ url}}
    {{# potentialAction}}
      "potentialAction": {
        {{# type}}"@type": "{{ . }}",{{/ type}}
        {{# target}} "target": "{{ . }}", {{/ target}}
        {{# query_input}} "query-input": "{{ . }}" {{/ query_input }}
      }
    {{/ potentialAction}}
  }
  </script>
{{/ js_slsb_ld }}

  {{# patternlab }}
    <!--DO NOT REMOVE-->
    {{{ patternLabFoot }}}
  {{/ patternlab }}
    </div>
  </body>
</html>
