{{#double-btns}}
<div class="btns-set">
    <span class="btn-padding">
        <a href="{{url-left}}" class="btn btn-left">
            <i class="fa fa-plus fa-fw"></i>{{btn-left-text}}
        </a>
        <a href="{{url-right}}" class="btn btn-right">
            <i class="fa fa-plus fa-fw"></i>{{btn-right-text}}
        </a>
    </span>
</div>
{{/double-btns}}
