{{> atoms/iconography/svgicons-search }}
{{> organisms/global/header_v2}}
<div role="main" class="search-object after-header-with-search">
  {{#content}}
    {{#helpbar}}
    <div class="helpbar">
      {{#tip}}
        <p class="helpbar-tip">{{{.}}}</p>
      {{/tip}}
      {{#tutorial}}
        <a class="helpbar-video" href="{{url}}">{{text}}</a>
      {{/tutorial}}
    </div>
    {{/helpbar}}
    {{#disabled}}
      {{#ugc_content}}
        {{#include_1418_nav}}
          {{>molecules/navigation/e7a1418-nav}}
        {{/include_1418_nav}}
      {{/ugc_content}}
    {{/disabled}}

    {{#object.media}}
      {{^items}}
        <section>
          <div class="object-media-info section is-current">
            <div class="object-downloads">
            </div>
          </div>
        </section>
      {{/items}}
    {{/object.media}}

    {{! Main object text info section }}

    <section class="section object-details has-right-column">
      <div class="channel-object-overview">
        {{#object}}
          <div>
            {{#media}}

              <div class="channel-media-wrap {{#empty_item}} empty{{/empty_item}}{{^empty_item}} multi-item{{/empty_item}}">

                {{#display_title}}
                  <h2 class="channel-object-title top" property="http://purl.org/dc/elements/1.1/title dc:title">{{.}}</h2>
                {{/display_title}}
                {{^display_title}}
                  <h2 class="channel-object-title top">{{i18n.site.object.title_unavailable}}</h2>
                {{/display_title}}

                <a class="scroll-trigger trigger-chain"
                   data-enabled="true"
                   data-disable-when-fired="showMediaThumbs"
                   data-fire-on-open-params='{ "trigger": "scroll-trigger-media-items"}'></a>

                <div class="object-media-viewer">
                  {{#empty_item}}

                    <div class="channel-object-no-preview">

                      {{#origin}}
                        {{#url}}
                          <span class="go-to-source-intro">
                            {{i18n.site.object.preview_unavailable_intro}}
                          </span>
                          <a class="go-to-source" href="{{.}}">{{i18n.site.object.preview_unavailable_source}}</a>
                        {{/url}}
                        {{#institution_name}}
                          <span class="go-to-source-explanation">
                            {{i18n.site.object.preview_unavailable_explanation}}
                            <a class="link-black-blue" href="{{url}}">{{.}}<span class="svg-icon-external"></span></a>
                          </span>
                        {{/institution_name}}
                      {{/origin}}

                    </div>
                  {{/empty_item}}
                  {{^empty_item}}
                    {{#items}}
                      {{#is_current}}
                        <div class="media-poster{{#playable}} contains-playable{{/playable}}">
                          <div {{#playable}}class="playable"{{/playable}} {{#play_url}}data-uri="{{.}}"{{/play_url}}>
                            <div class="zoomable">
                            </div>
                            <div class="media-options">

                              <div class="media-option-group-wrap">
                                <span class="media-option-group">
                                  <span class="media-option  media-share">
                                    <span class="share svg-icon-share"></span>
                                    <span class="media-option-label">Share</span>
                                  </span>
                                  <a class="media-option media-download" download="Europeana-Media">
                                    <span class="download svg-icon-download white"></span>
                                    <span class="media-option-label">Download</span>
                                  </a>
                                  <span class="media-option media-annotate">
                                    <span class="info svg-icon-info"></span>
                                    <span class="media-option-label">Info</span>
                                  </span>
                                </span>
                              </div>

                              <div class="media-option-group-wrap">
                                <span class="media-option-group">
                                  <span class="media-option media-zoom-in">
                                    <span class="zoom-in svg-icon-zoom-in"></span>
                                    <span class="media-option-label">Zoom in</span>
                                  </span>
                                  <span class="media-option media-zoom-out">
                                    <span class="zoom-out svg-icon-zoom-out"></span>
                                    <span class="media-option-label">Zoom out</span>
                                  </span>
                                </span>
                              </div>

                              <div class="media-option-group-wrap">
                                <span class="media-option-group">
                                  <span class="media-option  media-link">
                                    <span class="link svg-icon-link-out"></span>
                                    <span class="media-option-label">Link</span>
                                  </span>
                                </span>
                              </div>
                            </div>
                          </div>

                        </div>
                      {{/is_current}}
                    {{/items}}

                    {{#required_players}}

                      {{#iiif}}
                        <div class="object-media-iiif is-hidden">
                          {{> molecules/components/iiif/iiif }}
                        </div>
                      {{/iiif}}

                      {{#video}}
                        <div class="object-media-video is-hidden">
                          {{> molecules/components/videojs/videojs }}
                        </div>
                      {{/video}}

                      {{#midi}}
                        <div class="object-media-midi is-hidden">
                          {{> molecules/components/midi/midi }}
                        </div>
                      {{/midi}}

                      {{#oembed}}
                        <div class="object-media-oembed is-hidden" data-fullscreen-enter="{{i18n.global.actions.fullscreen-enter}}" data-fullscreen-exit="{{i18n.global.actions.fullscreen-exit}}">
                          {{> molecules/components/oembed/oembed }}
                        </div>
                      {{/oembed}}

                      {{#text}}
                        <div class="object-media-text is-hidden">
                          Text viewer here
                        </div>
                      {{/text}}

                      {{#audio}}
                        <div class="object-media-audio is-hidden">
                         {{> molecules/components/videojs/videojs }}
                        </div>
                      {{/audio}}

                      {{#pdf}}
                        <div class="object-media-pdf is-hidden">
                          {{> molecules/components/pdfjs/pdfjs-ui-thumbs-right }}
                        </div>
                      {{/pdf}}
                    {{/required_players}}
                  {{/empty_item}}

                  <div class="title-desc-media-nav">

                    {{#display_title}}
                      <h2 class="channel-object-title" property="http://purl.org/dc/elements/1.1/title dc:title">{{.}}</h2>
                    {{/display_title}}
                    {{^display_title}}
                      <h2 class="channel-object-title">{{i18n.site.object.title_unavailable}}</h2>
                    {{/display_title}}

                    <div class="channel-object-media-nav js-transparent">
                      {{> molecules/components/media-nav}}
                    </div>

                    {{#subtitle}}
                      <h3 class="subtitle">{{subtitle}}</h3>
                    {{/subtitle}}

                    <div class="channel-object-description cf">
                      {{#description}}
                        {{#sections}}
                          {{#items}}
                            <p>{{text}}</p>
                          {{/items}}
                          {{^items}}
                            <div class="missing">{{i18n.site.object.description_unavailable}}</div>
                          {{/items}}
                        {{/sections}}
                      {{/description}}
                    </div>

                    {{#view_more}}
                    <div class="channel-object-viewmore">
                      <a href="{{url}}" class="viewmore-image" style="background-image: url('{{img_url}}');"></a>
                      <h3>{{text}}</h3>
                    </div>
                    {{/view_more}}
                    {{^view_more}}
                      <script id="template-concept-js" type="text/x-mustache">
                        <div class="channel-object-viewmore">
                          <a href="[[url]]" class="viewmore-image" style="background-image: url('[[img_url]]');"></a>
                          <h3>[[text]]</h3>
                          <a href="[[url]]">{{i18n.object.actions.view-more}}</a>
                        </div>
                      </script>
                    {{/view_more}}

                  </div>
                </div>


                {{#empty_item}}
                  {{#thumbnail}}<img src={{.}}>{{/thumbnail}}
                  {{^thumbnail}}<br/><br/>{{/thumbnail}}
                {{/empty_item}}

              </div>
            {{/media}}
          </div>

          <div class="channel-object-information channel-object-action-bar">

            <div class="channel-object-media-actions">
              <div class="action-ctrl">
                <a class="action-ctrl-btn share" title="{{i18n.global.actions.share}}" data-modal-selector=".modal-share"><span class="action-icon svg-icon-share"></span><span class="action-label">{{i18n.global.actions.share}}</span></a>
              </div>

              <div class="action-ctrl">
                <a class="action-ctrl-btn media-download {{#media}}{{#items}}{{#is_current}}{{^downloadable}} disabled {{/downloadable}}{{/is_current}}{{/items}}{{/media}}"
                   {{#media}}{{#items}}{{#is_current}}{{#download}} href="{{url}}" {{/download}}{{/is_current}}{{/items}}{{/media}}
                   title="{{i18n.site.object.actions.download}}"
                   data-modal-selector=".modal-download"
                   download="Europeana-Media"><span class="action-icon svg-icon-download"></span><span class="action-label">{{i18n.site.object.actions.download}}</span></a>
              </div>

              {{#rights}}
                <div class="action-ctrl object-rights">
                  <h4>{{ i18n.global.facet.header.reusability }}</h4>
                  <div class="object-license child-link-black-blue">{{license_brief}} {{> molecules/icons/licenses-icon-only }}</div>
                </div>
              {{/rights}}
              {{> molecules/controls/channels-object-origin }}
            </div>

            <script id="template-modal-header-js" type="text/x-mustache">
              <div class="modal-header none">
                <a class="action-ctrl-btn" data-modal-selector=".modal-share"><span>{{i18n.global.share}}</span></a>
                <a class="action-ctrl-btn[[^data-download-uri]] disabled[[/data-download-uri]]" data-modal-selector=".modal-download" download="Europeana-Media" href="[[data-download-uri]]"><span>{{i18n.site.object.actions.download}}</span></a>
                <a class="action-ctrl-btn" data-modal-selector=".modal-rights"><span>{{i18n.global.facet.header.reusability}} [[license_brief]]</span></a>
                {{> molecules/controls/channels-object-origin }}
              </div>
            </script>

            <script id="template-modal-rights-js" type="text/x-mustache">
              <div class="action-modal modal-rights js-hidden">
                <div class="modal-content">
                  <div class="modal-action">
                    <span class="label">
                      <h4>{{ i18n.global.facet.header.reusability }}</h4>
                      <label>[[license_brief]]</label>
                      <h4>[[#with_attribution]]{{i18n.global.facet.reusability.with-attribution}}[[/with_attribution]]</h4>
                    </span>
                  </div>
                  <div class="action-modal-section">
                    <a target="blank" href="[[license_url]]" class="link-black-blue license-link-full"> {{> molecules/icons/licenses-icon-only-js }} [[license_name]] <span class="svg-icon-external"></span></a>
                  </div>
                  <a class="media-modal-close">
                    <span class="label"></span>
                  </a>
                </div>
              </div>
            </script>

            <script id="template-modal-download-js" type="text/x-mustache">

              <div class="action-modal modal-download js-hidden">
                <div class="modal-content">
                  <div class="modal-action">
                    <span class="label">
                        Downloading
                        <span class="label-small">You can click <a download="Europeana-Media" href="[[data-download-uri]]">here</a> to try again</span>
                      </span>
                  </div>

                  <div class="action-modal-section">
                    <span class="file">[[data-mime-type]]</span>
                    <ul>
                      <li><span class="action-modal-subheading">format</span> [[data-format]]</li>
                      <li><span class="action-modal-subheading">file size</span> [[data-file-size]] [[data-file-unit]]</li>
                    </ul>
                  </div>

                  <div class="action-modal-section">
                    <span class="attribution-heading">Show your appreciation. Copy to attribute.</span>
                    <input value="[[data-attribution-plain]]">
                  </div>

                  <div class="action-modal-section centred">
                    <span class="action-modal-subheading">other formats</span>
                    [[formats]]
                    <ul>
                      <li><a href="[[url]]">[[text]]</a></li>
                    </ul>
                    [[formats]]
                  </div>

                  <a class="media-modal-close">
                    <span class="label"></span>
                  </a>
                </div>
              </div>
            </script>

            <div class="action-modal modal-share js-hidden">

              <div class="modal-content">
                <div class="modal-action">
                  <span class="label">
                    {{i18n.global.share}}
                  </span>
                </div>

                {{#social_share }}
                  {{> molecules/controls/share-social }}
                {{/social_share }}

                <a class="media-modal-close">
                  <span class="label"></span>
                </a>
              </div>

            </div>
          </div>

          <div class="channel-object-information">
            {{#people}}
              {{> organisms/sections/object-data-section }}
            {{/people}}

            {{#concepts }}
              {{> organisms/sections/object-data-section }}
            {{/concepts }}
          </div>

          <div class="channel-object-extended-information">

            <h2 class="channel-object-title" data-label-expand="[{{i18n.site.object.actions.expand-extended}}]" data-label-collapse="[{{i18n.site.object.actions.collapse-extended}}]">  Extended Information</h2>

            {{#properties }}
              {{> organisms/sections/object-data-section }}
            {{/properties }}

            {{#dates }}
              {{> organisms/sections/object-data-section }}
            {{/dates }}

            {{#places }}
              {{> organisms/sections/object-data-section }}
            {{/places }}

            {{#annotations_later}}
              <script id="js-template-object-data-section" type="text/x-mustache">{{> organisms/sections/object-data-section-js }}</script>
            {{/annotations_later}}

            {{#annotations}}
              {{> organisms/sections/object-data-section }}
            {{/annotations}}

            {{#provenance}}
              {{> organisms/sections/object-data-section }}
            {{/provenance}}

            {{#copyright}}
              {{> organisms/sections/object-data-section }}
            {{/copyright}}

            {{#refs_rels}}
              {{> organisms/sections/object-data-section }}
            {{/refs_rels}}

            {{#meta_additional}}
              {{#present}}
                <div class="data-section cf closed" {{#id}} data-section-id="{{.}}"{{/id}}>
                  <h3 class="subsection-label">
                    {{i18n.site.object.meta-label.location}}
                  </h3>
                  <div class="subsection-content">
                    {{#places}}
                      <ul class="data-group">
                        {{#sections}}
                          <li>
                            <section>
                              <h4 class="data-header">{{#title }}{{.}}:{{/title }}</h4>
                              <ul class="comma-list data-group">
                                {{# items }}
                                  <li>
                                    {{#url }}<a href="{{.}}">{{/url }}{{ text }}{{#url }}</a>{{/url }}{{#extra_info}}
                                      {{#life}}
                                        <span class="life-dates">
                                          ({{from.short}} {{#to.short}} â€” {{.}}{{/to.short}})
                                        </span>
                                      {{/life}}
                                      {{#biography}}
                                        <div class="biography">
                                          {{text}}
                                        </div>
                                      {{/biography}}
                                    {{/extra_info}}
                                  </li>
                                {{/ items}}
                              </ul>
                            </section>
                          </li>
                        {{/sections}}
                      </ul>
                    {{/places}}

                    {{#geo.long_and_lat}} {{! check both present }}
                      <a class="scroll-trigger"
                         id="scroll-trigger-map"
                         data-enabled="true"
                         data-fire-on-open="showMap"
                         data-fire-on-open-params='{
                           "longitude":{{geo.longitude}}, "latitude": {{geo.latitude}}, "labels": { "n":"{{geo.labels.points.n}}", "s":"{{geo.labels.points.s}}", "e":"{{geo.labels.points.e}}", "w":"{{geo.labels.points.w}}" }
                         }'
                        ></a>
                      <span id="js-map-place-name" class="is-hidden">{{geo.placeName}}</span>
                      <a id="map" style="bottom:4.8em; position: relative;"></a>
                      <div class="map">
                        <div><b>{{geo.labels.longitude}}</b> {{#geo.longitude}}{{.}}{{/geo.longitude}}</div>
                        <div><b>{{geo.labels.latitude}}</b> {{#geo.latitude}}{{.}}{{/geo.latitude}}</div>
                      </div>
                      <a class="scroll-trigger trigger-chain"
                        data-enabled="true"
                        data-disable-when-fired="showMap"
                        data-fire-on-open-params='{"trigger": "scroll-trigger-map"}'></a>
                    {{/geo.long_and_lat}}
                  </div>
                </div>
              {{/present}}
            {{/meta_additional}}

            <div class="data-section closed" data-section-id="hierarchy">
              <div class="hierarchy cf">
                <div class="subsection-label">
                  {{i18n.site.object.hierarchy.title}}
                </div>
                <div class="subsection-content">
                  <div class="hierarchy-objects" style="display:none;"></div>
                </div>
              </div>
            </div>

            {{#content.named_entities}}
              <div class="data-section closed" data-section-id="named-entities">
                <div class="cf">
                  <div class="subsection-label">
                    {{i18n.site.object.named-entities.title}}
                  </div>
                  <div class="subsection-content">
                    {{>molecules/navigation/foldable-list}}
                  </div>
                </div>
              </div>
            {{/content.named_entities}}

          </div>

        {{/object}}

      </div>

      {{! end of object info column }}

      {{> organisms/cards/js-template-promo-entity}}

      {{> organisms/cards/js-template-promo-exhibition}}

      {{> organisms/cards/js-template-promo-gallery}}

      {{> organisms/cards/js-template-promo-generic}}

      {{> organisms/cards/js-template-promo-next-prev}}

      {{> organisms/cards/js-template-promo-news}}

      <div class="channel-object-actions">
        <div class="slide-rail">
          {{> organisms/cards/template-promo-preload }}
        </div>
        <div class="eu-slide-navs">
          <a class="svg-icon-previous carousel-arrows blue-black    eu-slide-nav eu-slide-nav-left disabled">&nbsp;</a>
          <a class="svg-icon-next carousel-arrows blue-black    eu-slide-nav eu-slide-nav-right disabled">&nbsp;</a>
        </div>
      </div>

      {{! end object actions }}

      {{#object.suggestions}}
        <div class="channel-object-similar">
          <h2 class="channel-object-title">[More Suggestions]</h2>
          <div class="suggestions-section">
            <div class="display-grid">
              <div class="slide-rail" style="position: relative;">
                <script id="template-preview-tab-content" type="text/x-mustache">
                  <article class="suggestion-item">
                    <div class="item-preview">
                      <div class="item-image">
                        <div class="inner">
                          <a target="[[target]]" href="[[url]]" tabindex="-1">
                            <div class="mlt-img-div height-to-width" [[#img.src]]style="background-image:url('[[.]]');"[[/img.src]]></div>
                          </a>
                        </div>
                      </div>
                    </div>
                    <div class="item-info">
                      {{! no whitespace here - eu-ellipsis js to be applied }}
                      <h2><a class="link-black-blue" [[#url]]href="[[.]]"[[/url]] [[#target]]target="[[.]]"[[/target]]>[[#title]]<span class="link-text">[[.]]</span>[[#external]]<span title="[[title]]" class="svg-icon-external"></span>[[/external]][[/title]]</a></h2>
                    </div>
                  </article>
                </script>
                <div class="eu-accordion-tabs suggestions cf">
                  {{#tab_items}}
                    {{> organisms/components/eu-accordion-tabs-portal-suggestions}}
                  {{/tab_items}}
                  {{#tab_items}}
                    <h4 class="more-federated-results js-hidden js-empty"><a href="" class="svg-icon-external-eu-blue-after"></a></h4>
                  {{/tab_items}}
                </div>
              </div>
              <a class="svg-icon-previous carousel-arrows blue-black    eu-slide-nav eu-slide-nav-left disabled">&nbsp;</a>
              <a class="svg-icon-next carousel-arrows blue-black    eu-slide-nav eu-slide-nav-right disabled">&nbsp;</a>
            </div>
          </div>
        </div>
      {{/object.suggestions}}
    </section>
  {{/content}}
</div>
{{> organisms/sections/banner }}
{{> organisms/global/footer_v2 }}
{{> molecules/forms/feedback }}
