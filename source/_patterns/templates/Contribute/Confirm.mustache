{{> atoms/iconography/svgicons-search }}
{{> organisms/global/header_v2}}

<div role="main" id="maincontent">

<section class="search-object">
  <div class="object-nav cf">
    <div class="object-nav-lists">
      <ul class="breadcrumbs">
        <li><a href="{{navigation.home_url}}"><span class="svg-icon-home"></span>{{i18n.site.navigation.breadcrumb.return_home}}</a></li>
        <li><span class="lastitem"><strong>{{content.title}}</strong></span></li>
      </ul>
    </div>
  </div>
</section>

<section class="static-header">
  <div class="lc">
    {{#content.title}}
      <h1 class="title">{{.}}</h1>
    {{/content.title}}
  </div>
</section>

<section class="ugc-content section">
  <div class="lc">
    
    {{> molecules/ugc/log-out-link }}

    {{#content}}

    {{#flash_notice}}
      <strong class="flash-notice">{{.}}</strong>
    {{/flash_notice}}

    {{#confirmation_text}}
      <h1 class="ugc-confirm-header">{{.}}</h1>
    {{/confirmation_text}}

    {{/content}}

    {{#navigation.secondary}}
      <div class="sidebar js-getstickywidth">
         <div class="js-sticky">
        {{> molecules/navigation/nav-secondary }}
        </div>
      </div>
    {{/navigation.secondary}}

    {{#content}}
      {{#form}}
        <div class="lc">
          {{{.}}}
        </div>
      {{/form}}
    {{/content}}
  </div>
</section>

</div>
<!-- End Main -->

{{> organisms/sections/banner }}

{{> organisms/global/footer_v2 }}
{{> molecules/forms/feedback }}
