{{> atoms/iconography/svgicons-search }}
{{> organisms/global/header_v2}}

<div role="main" id="maincontent">

<section class="search-object">
  <div class="object-nav cf">
    <div class="object-nav-lists">
      <ul class="breadcrumbs">
        <li><a href="{{navigation.home_url}}"><span class="svg-icon-home"></span>{{i18n.site.navigation.breadcrumb.return_home}}</a></li>
        <li><span class="lastitem"><strong>{{content.title}}</strong></span></li>
      </ul>
    </div>
  </div>
</section>

<section class="static-header">
  <div class="lc">
    {{#content.title}}
      <h1 class="title">{{.}}</h1>
    {{/content.title}}
  </div>
</section>

<section class="ugc-content section">

  {{> molecules/ugc/log-out-link }}

  <div class="lc">

    {{#content}}

    {{#flash_notice}}
      <strong class="flash-notice">{{.}}</strong>
    {{/flash_notice}}

    {{/content}}

    {{#navigation.secondary}}
      <div class="sidebar js-getstickywidth">
         <div class="js-sticky">
        {{> molecules/navigation/nav-secondary }}
        </div>
      </div>
    {{/navigation.secondary}}

    {{#content.resources}}
      {{#has_campaigns}}
        <select class="contribution-select" autocomplete="off" onchange="if(this.value){location.href=this.value}else{location.href=location.href.substr(0, location.href.indexOf('?'))}">
          {{#campaigns}}
            <option value="{{url}}"{{#is_selected}} selected{{/is_selected}}>{{label}}</option>
          {{/campaigns}}
        </select>
      {{/has_campaigns}}
      {{#has_events}}
        <select class="contribution-select" autocomplete="off" onchange="if(this.value){location.href=this.value}else{location.href=location.href.substr(0, location.href.indexOf('?'))}">
          {{#events}}
            <option value="{{url}}"{{#is_selected}} selected{{/is_selected}}>{{label}}</option>
          {{/events}}
        </select>
      {{/has_events}}

      {{#create}}
        {{#url}}
          <a class="create-new-link" href="{{.}}">{{text}}</a>
        {{/url}}
      {{/create}}

      {{#form}}
        <form {{#attributes}}{{#name}}{{#value}}{{name}}={{.}} {{/value}}{{/name}}{{/attributes}}>
      {{/form}}

        {{#table}}
          {{> organisms/data/data-table }}
        {{/table}}

      {{#form}}
        <div class="submits">
          {{#actions}}
            <input type="submit" onClick="this.form.action='{{action}}'" value="{{label}}" class="btn inline-form-button submit">
          {{/actions}}
        </div>
        </form>
      {{/form}}

    {{/content.resources}}

  </div>
</section>

</div>
<!-- End Main -->

{{> organisms/sections/banner }}

{{> organisms/global/footer_v2 }}
{{> molecules/forms/feedback }}
